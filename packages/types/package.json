{
	"name": "@rushcms/types",
	"version": "0.1.1",
	"description": "TypeScript type definitions for RushCMS SDK",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rush-cms/sdk.git",
        "directory": "packages/types"
    },
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"dev": "tsup src/index.ts --format cjs,esm --dts --watch",
		"clean": "rm -rf dist",
		"lint": "eslint src"
	},
	"keywords": [
		"rushcms",
		"typescript",
		"types",
		"cms"
	],
	"license": "MIT",
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.3"
	}
}
