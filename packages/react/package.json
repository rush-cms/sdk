{
  "name": "@rushcms/react",
  "version": "0.1.7",
  "description": "React components and hooks for RushCMS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rush-cms/sdk.git",
    "directory": "packages/react"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "keywords": [
    "rushcms",
    "react",
    "components",
    "cms"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@rushcms/client": "workspace:*",
    "@rushcms/types": "workspace:*",
    "clsx": "^2.1.1",
    "photoswipe": "^5.4.4",
    "react-photoswipe-gallery": "^4.0.0",
    "react-syntax-highlighter": "^16.1.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}